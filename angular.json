{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "angular-chat-app": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/angular-chat-app",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                       
                        "assets": [
                          "src/favicon.ico",
                          "src/assets",
                          "src/firebase-messaging-sw.js",
                          "src/manifest.json",
                          "src/manifest.webmanifest"
                        ],
                        "styles": [
                            "@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.css",
                            "./src/assets/css/lib/bootstrap.min.css",
                            "./src/assets/css/swipe.min.css",
                            "./node_modules/ngx-toastr/toastr.css"
                        ],
                        "scripts": [
                            "./src/assets/js/vendor/popper.min.js",
                            "./src/assets/js/bootstrap.min.js",
                            "./src/assets/js/vendor/jquery-slim.min.js",
                            "./src/firebase-messaging-sw.js"
                        ],
                        "serviceWorker": true,
                        "ngswConfigPath": "ngsw-config.json"
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "10mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "10mb",
                                    "maximumError": "10mb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "angular-chat-app:build:production"
                        },
                        "development": {
                            "browserTarget": "angular-chat-app:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "angular-chat-app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "assets": [
                          "src/favicon.ico",
                          "src/assets",
                          "src/manifest.webmanifest"
                        ],
                        "styles": [
                            "@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.css"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": "fe4715b8-7929-4ac7-a6f8-6648348ebf29"
    }
}
